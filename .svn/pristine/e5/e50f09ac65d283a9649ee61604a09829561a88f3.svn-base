package cn.mycloudedu.user.domain

import cn.mycloudedu.framework.core.annotations.OrderProperty

import scala.beans.BeanProperty

/**
  * Created by e诺
  * on 2017/4/20
  * Time 下午3:49
  */
class Position {
  @BeanProperty var id: Integer = null
  @BeanProperty var companyId: Integer = null
  @BeanProperty var title: String = null
  @BeanProperty var description: String = null
  @BeanProperty var typeId: Integer = null
  @BeanProperty var specialNote: String = null
  @BeanProperty var tag: String = null
  @BeanProperty var property: Integer = null
  @BeanProperty var experience: Integer = null
  @BeanProperty var degree: Integer = null
  @BeanProperty var minSalary: Integer = null
  @BeanProperty var maxSalary: Integer = null
  @BeanProperty var amount: Integer = null
  @BeanProperty var status: Integer = null
  @OrderProperty
  @BeanProperty var updateTime: Long = 0
  @BeanProperty var creator: Integer = null
  @BeanProperty var createTime: Long = 0
  @BeanProperty var province: Integer = null
  @BeanProperty var city: Integer = null
  @BeanProperty var district: Integer = null
  @BeanProperty var address: String = null
  @BeanProperty var occupy: Integer = null
}
