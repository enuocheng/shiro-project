package cn.mycloudedu.user.dto

import scala.beans.BeanProperty

/**
  * Created by e诺
  * on 2017/5/4
  * Time 上午9:11
  */
class ResumeBaseDTO extends Serializable{
  @BeanProperty var id: Integer = null
  @BeanProperty var avatar: String = null
  @BeanProperty var name: String = null
  @BeanProperty var sex: Integer = null
  @BeanProperty var basePercent: Integer = null
  @BeanProperty var birthday: Long = 0
  @BeanProperty var provinceId: Integer = null
  @BeanProperty var provinceName: String = null
  @BeanProperty var cityId: Integer = null
  @BeanProperty var cityName: String = null
  @BeanProperty var education: Integer = null
  @BeanProperty var mobile: String = null
  @BeanProperty var email: String = null
  @BeanProperty var advantage: String = null
  @BeanProperty var expectPercent: Integer = 0 //求职意向完善度
  @BeanProperty var internshipPercent: Integer = 0 //实习经历完善度
  @BeanProperty var projectPercent: Integer = 0 //项目经验完善度
  @BeanProperty var educationPercent: Integer = 0 //教育经历完善度
  @BeanProperty var samplePercent: Integer = 0 //作品信息完善度
}
