package cn.mycloudedu.user.domain

import cn.mycloudedu.framework.core.annotations.OrderProperty

import scala.beans.BeanProperty

/**
  * Created by e诺
  * on 2017/4/18
  * Time 上午10:37
  */
class Daily {
  @BeanProperty var id: Integer = null
  @BeanProperty var userId: Integer = null
  @BeanProperty var projectId: Integer = null
  @BeanProperty var title: String = null
  @BeanProperty var content: String = null
  @BeanProperty var scope: Integer = null
  @BeanProperty var images: String = null
  @BeanProperty var status: Integer = null
  @OrderProperty
  @BeanProperty var createTime: Long = 0
  @BeanProperty var updateTime: Long = 0
  @OrderProperty
  @BeanProperty var publishTime: Long = 0
}
