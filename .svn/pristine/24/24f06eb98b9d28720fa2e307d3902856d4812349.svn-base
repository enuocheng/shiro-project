package cn.mycloudedu.user.param

import scala.beans.BeanProperty

/**
  * Created by e诺
  * on 2017/5/3
  * Time 上午9:29
  */
class SearchPositionParam extends Serializable{
  @BeanProperty var search: String = null
  @BeanProperty var province: Integer = null
  @BeanProperty var city: Integer = null
  @BeanProperty var property: Integer = null //工作性质 0 全职；1 实习； 2千生计划
  @BeanProperty var degree: Integer = null //0: 不限1: 大专2: 本科3: 硕士4: 博士
  @BeanProperty var minSalary: Integer = null
  @BeanProperty var maxSalary: Integer = null
  @BeanProperty var stage: Integer = null //0 天使轮,1 A ,2 B, 3 C,4 D+,5 不需要融资,6 已上市'
  @BeanProperty var scale: Integer = null //0 少于50人 1 50-150人 2 150-500人 3 500-1000人 4 1000-5000人 5 5000-10000人 6 10000人以上
}
